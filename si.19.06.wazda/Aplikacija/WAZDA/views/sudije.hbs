<body>
  <div id="wrapper" class="boxed">
    <!-- (+) -->
    <div class="hidden-top">
      <div class="hidden-top-inner container">
        <div class="row">
          <div class="span12" style="text-align:center">
            <ul>
              <li><strong>Kontaktiraj nas na:</strong></li>
              <li>Sedište: Trg Pavla Stojkovića 10, 18000 Niš</li>
              <li>Tel/faks: <i class="icon-phone"></i>018/516-605</li>
            </ul>
          </div>
        </div>
      </div>
    </div>
    <!-- end  -->

    <!-- start header -->
    <header>
      <div class="container">
        <!-- skriveni (+) kontakt podaci -->
        <div id="header-hidden-link">
          <a href="#" class="toggle-link" title="Click me you'll get a surprise"
            data-target=".hidden-top"><i></i>Otvori</a>
        </div>
        <!-- end  -->

        <div class="row nomargin">
          <div class="span12">
            <div class="headnav">
              <ul>
                <!-- <li><a href="#mySignup" data-toggle="modal"><i class="icon-user"></i> Sign up</a></li> -->
                {{#if isAuthenticated}}
                <li><a href="/logout">Odjavi se</a></li>
                {{else}}
                <li><a href="#mySignin" data-toggle="modal">Prijavi se</a></li>
                {{/if}}
              </ul>
            </div>

            <!-- PRIJAVI SE -->
            <div id="mySignin" class="modal styled hide fade" tabindex="-1" role="dialog"
              aria-labelledby="mySigninModalLabel" aria-hidden="true">
              <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-hidden="true">×</button>
                <h4 id="mySigninModalLabel">Uloguj se </h4>
              </div>
              <div class="modal-body">
                <div class="span6">
                  <h4>Clan <strong>saveza</strong></h4>
                  <ul class="nav nav-tabs">
                    <li class="active"><a href="#one" data-toggle="tab"><i class="icon-flag"></i> Sudija</a></li>
                    <li><a href="#two" data-toggle="tab"><i class="icon-user"></i>Delegat</a></li>
                    <li><a href="#three" data-toggle="tab"><i class="icon-briefcase"></i>Sekretar</a></li>
                    <li><a href="#four" data-toggle="tab"><i class="icon-lock"></i>Admin</a></li>
                  </ul>
                  <div class="tab-content">
                    <div class="tab-pane active" id="one">
                      <form class="form-horizontal" method="POST" action="/loginSudija">
                        <h3><i class="icon-flag icon-48"></i>Sudija</h3>
                        <div class="control-group">
                          <label class="control-label" for="inputText">Korisnicko ime</label>
                          <div class="controls">
                            <input name="username" placeholder="Korisnicko ime">
                          </div>
                        </div>
                        <div class="control-group">
                          <label class="control-label" for="inputSigninPassword">Šifra</label>
                          <div class="controls">
                            <input type="password" name="password" placeholder="Šifra">
                          </div>
                        </div>
                        <div class="control-group">
                          <div class="controls">
                            <button id="prijaviSe" class="btn">Uloguj se</button>
                          </div>
                        </div>
                      </form>
                    </div>
                    <div class="tab-pane" id="two">
                      <form class="form-horizontal" method="POST" action="/loginDelegat">
                        <h3><i class="icon-user icon-48"></i>Delegat</h3>
                        <div class="control-group">
                          <label class="control-label" for="inputText">Korisnicko ime</label>
                          <div class="controls">
                            <input name="username" placeholder="Korisnicko ime">
                          </div>
                        </div>
                        <div class="control-group">
                          <label class="control-label" for="inputSigninPassword">Šifra</label>
                          <div class="controls">
                            <input type="password" name="password" placeholder="Šifra">
                          </div>
                        </div>
                        <div class="control-group">
                          <div class="controls">
                            <button id="prijaviSe" class="btn">Uloguj se</button>
                          </div>
                        </div>
                      </form>
                    </div>
                    <div class="tab-pane" id="three">
                      <form class="form-horizontal" method="POST" action="/loginSekretar">
                        <h3><i class="icon-briefcase icon-48"></i>Sekretar</h3>
                        <div class="control-group">
                          <label class="control-label" for="inputText">Korisnicko ime</label>
                          <div class="controls">
                            <input name="username" placeholder="Korisnicko ime">
                          </div>
                        </div>
                        <div class="control-group">
                          <label class="control-label" for="inputSigninPassword">Šifra</label>
                          <div class="controls">
                            <input type="password" name="password" placeholder="Šifra">
                          </div>
                        </div>
                        <div class="control-group">
                          <div class="controls">
                            <button id="prijaviSe" class="btn">Uloguj se</button>
                          </div>
                        </div>
                      </form>
                    </div>
                    <div class="tab-pane" id="four">
                      <form class="form-horizontal" method="POST" action="/loginAdmin">
                        <h3><i class="icon-lock icon-48"></i>Admin</h3>
                        <div class="control-group">
                          <label class="control-label" for="inputText">Korisnicko ime</label>
                          <div class="controls">
                            <input name="username" placeholder="Korisnicko ime">
                          </div>
                        </div>
                        <div class="control-group">
                          <label class="control-label" for="inputSigninPassword">Šifra</label>
                          <div class="controls">
                            <input type="password" name="password" placeholder="Šifra">
                          </div>
                        </div>
                        <div class="control-group">
                          <div class="controls">
                            <button id="prijaviSe" class="btn">Uloguj se</button>
                          </div>
                        </div>
                      </form>
                    </div>
                  </div>
                </div>
              </div>
            </div>
            <!-- end  -->
          </div>
        </div>

        <div class="row">
          <div class="span4">
            <div class="logo">
              <a id="logoPocetna" href="http://localhost:3000/"><img src="img/fsn.jpg" alt="" class="logo"
                  style="height:100px;" /></a>
              <h1><strong>Fudbalski savez Niša</strong></h1>
            </div>
          </div>
          <div class="span8">
            <div class="navbar navbar-static-top">
              <div class="navigation">
                <nav>
                  <ul class="nav topnav">
                    {{#if isAuthenticated}}
                    <li class="dropdown active">
                      <a id="profil" href="http://localhost:3000/vratiOdredjeniProfil">Profil</a>
                    </li>
                    {{/if}}
                    <li class="dropdown ">
                      <a id="pocetna" href="http://localhost:3000/">Početna</a>
                    </li>
                    <li>
                      <a id="oNama" href="http://localhost:3000/onama">O nama</a>
                    </li>
                    <li class="dropdown">
                      <a href="#">Sudije <i class="icon-angle-down"></i></a>
                      <ul class="dropdown-menu">
                        <li>
                          <a id="organizacija" href="http://localhost:3000/organizacija">Organizacija sudija</a>
                        </li>
                        <li>
                          <a id="listaSudija" href="http://localhost:3000/listaSudija">Lista sudija</a>
                        </li>
                        <li>
                          <a id="dokumenti" href="http://localhost:3000/dokumenta">Dokumenti</a>
                        </li>
                      </ul>
                    </li>
                    <li class="dropdown">
                      <a id="delegati" href="http://localhost:3000/delegati">Delegati</a>
                    </li>
                    <li class="dropdown">
                      <a href="#">Lige <i class="icon-angle-down"></i></a>
                      <ul class="dropdown-menu">
                        <li>
                          <a id="prvaLiga" href="http://localhost:3000/prvaLiga">Prva niška liga</a>
                        </li>
                        <li>
                          <a id="drugaLiga" href="http://localhost:3000/drugaLiga">Druga Niška liga</a>
                        </li>
                        <li>
                          <a id="mladjaLiga" href="http://localhost:3000/mladjaLiga">Mlađe kategorije</a>
                        </li>
                        <li>
                          <a id="cicibani" href="http://localhost:3000/cicibani">Cicibani</a>
                        </li>
                      </ul>
                    </li>
                    <li>
                      <a id="tereni" href="http://localhost:3000/tereni">Tereni</a>
                    </li>
                  </ul>
                </nav>
              </div>
              <!-- end-->
            </div>
          </div>
        </div>
      </div>
    </header>
    <!-- end header -->

    <section id="inner-headline">
      <div class="container">
        <div class="row">
          <div class="span4">
            <div class="inner-heading">
              <h2>Lista sudija</h2>
            </div>
          </div>
          <div class="span8">
            <ul class="breadcrumb">
              <li>
                <a id="iconPocetna2" href="http://localhost:3000/"><i class="icon-home"></i></a><i
                  class="icon-angle-right"></i>
              </li>
              <li>Sudije<i class="icon-angle-right"></i></li>
              <li class="active">Lista sudija</li>
            </ul>
          </div>
          <div class="span12 poravnaj">
            <i class="icon-search icon-48" style="margin-top:15px"></i><input style="margin-top:10px" id="pretraga"
              type="text" placeholder="Search" />
            <ul class="breadcrumb" style="margin-top:0px">
              <li>
                <button id="dugmePrve" class="btn btn-inverse" style="margin-left:5px">
                  <i class="icon-user"></i> Sudije Prve lige
                </button>
              </li>
              <li>
                <button id="dugmeDruge" class="btn btn-inverse" style="margin-left:5px">
                  <i class="icon-user"></i> Sudije Druge lige
                </button>
              </li>
              <li>
                <button id="dugmeMladjih" class="btn btn-inverse" style="margin-left:5px">
                  <i class="icon-user"></i> Sudije Mlađih kategorija
                </button>
              </li>
              <li>
                <button id="dugmeCicibani" class="btn btn-inverse" style="margin-left:5px">
                  <i class="icon-user"></i> Sudije Cicibana
                </button>
              </li>
            </ul>
          </div>
        </div>
      </div>
    </section>
    <section id="content">
      <div class="container">
        <div class="row">
          <div class="span4">
            <aside class="left-sidebar">
              <div class="widget"></div>
              <div class="widget">
                <h5 class="widgetheading">Kategorije</h5>
                <ul class="cat">
                  <li>
                    <i class="icon-angle-right"></i><a href="#">Sudije</a>
                  </li>
                  <li>
                    <i class="icon-angle-right"></i><a id="delegati2" href="http://localhost:3000/delegati">Delegati</a>
                  </li>
                  <li>
                    <i class="icon-angle-right"></i><a id="prvaLiga2" href="http://localhost:3000/prvaLiga">Prva Niška
