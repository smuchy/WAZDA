                  </li>
                  <li>
                    <i class="icon-angle-right"></i><a id="drugaLiga2" href="http://localhost:3000/drugaLiga">Druga
                      Niška liga</a>
                  </li>
                  <li>
                    <i class="icon-angle-right"></i><a id="mladjaLiga2" href="http://localhost:3000/mladjaLiga">Mlađe
                      kategorije</a>
                  </li>
                  <li>
                    <i class="icon-angle-right"></i><a id="cicibani2" href="http://localhost:3000/cicibani">Liga
                      cicibana</a>
                  </li>
                </ul>
              </div>
            </aside>
          </div>
          <div class="span8">
            <article>
              <div class="row">
                <div class="span8">
                  <section id="sudije">
                    <div id="grid-container"></div>
                  </section>
                </div>
              </div>
            </article>
          </div>
        </div>
      </div>
    </section>

    <footer>
      <!-- sub-footer -->
      <div class="container">
        <div class="row">
          <div class="span7">
            <div class="copyright">
              <p>
                <span>&copy; Zvanična internet prezentacija FS Niša. Trg Pavla
                  Stojkovića 10, 18000 Niš Tel/faks:018/516-605; ©
                  fsn.org.rs</span>
              </p>
              <div class="credits">
                Designed by <a href="#">inFiNiteMiNds</a>
              </div>
            </div>
          </div>
          <div class="span5">
            <ul class="social-network">
              <li>
                <a href="https://www.facebook.com/fsnisa/" data-placement="bottom" title="Facebook"><i
                    class="icon-facebook icon-square"></i></a>
              </li>
            </ul>
          </div>
        </div>
      </div>
